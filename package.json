{
  "name": "@egomobile/azuread-auth",
  "version": "0.1.2",
  "description": "A middleware, which wraps functions of passport-azure-ad module, written for passport.",
  "main": "lib/index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "lib",
    "!lib/**/*.map",
    "README.md"
  ],
  "keywords": [
    "node",
    "js",
    "ts",
    "javascript",
    "typescript",
    "oauth",
    "oauth2",
    "bearer",
    "bearer stradegy",
    "azure",
    "azuread",
    "azure active directory",
    "passport",
    "ego"
  ],
  "scripts": {
    "build": "del ./lib && tsc",
    "docs:generate": "del ./docs && typedoc --out ./docs ./src",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
    "sandbox": "nodemon --watch './sandbox.ts' ./sandbox.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/egomobile/node-azuread-auth"
  },
  "author": "Next.e.GO Mobile SE, Aachen, Germany",
  "license": "LGPL-3.0",
  "dependencies": {
    "@types/node": "12.20.33",
    "@types/passport": "1.0.7",
    "@types/passport-azure-ad": "4.3.1",
    "passport": "0.5.2",
    "passport-azure-ad": "4.3.1"
  },
  "devDependencies": {
    "@egomobile/http-server": "^0.33.1",
    "@egomobile/tsconfig": "^5.0.0",
    "del-cli": "4.0.1",
    "dotenv": "14.3.2",
    "eslint": "7.32.0",
    "eslint-config-ego": "^0.15.0",
    "nodemon": "2.0.15",
    "ts-node": "10.4.0",
    "tslint": "6.1.3",
    "typedoc": "0.22.11",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "@egomobile/http-server": "^0.33.1"
  }
}
